{
  "compilerOptions": {
                                 /* Language and Environment */
"target": "es2016",

/* Modules */
"module": "commonjs",
"esModuleInterop": true,
"allowSyntheticDefaultImports": true,
"resolveJsonModule": true,

"forceConsistentCasingInFileNames": true,

/* Type Checking */
"strict": true,

/* Completeness */
"skipLibCheck": true
  }
}
